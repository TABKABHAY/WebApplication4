@{ Layout = null; }



<head>
    <link rel="stylesheet" href="~/css//ServiceProviderDeshboard.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.2/main.min.css">

</head>
@section Custom
 {
    <link rel="stylesheet" href="~/css//ServiceProviderDeshboard.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.2/main.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
}


<h1>Welcome <span> @ViewBag.Name </span></h1>

<section class="main_service clearfix">

    <!-- tab -->


    <div class="tab" id="verticaltab">


        <button class="tablinks" onclick="Sp_TabNav(event, 'Dashboard')" id="DashboardTabBtn">Dashboard</button>


        <button class="tablinks" onclick="Sp_TabNav(event, 'NewServiceRequest')" id="NewServiceRequestTabBtn">


            New Service Request


        </button>
        <button class="tablinks" onclick="Sp_TabNav(event, 'UpcomingService')" id="UpcomingServiceTabBtn">


            Upcoming Service


        </button>

        <button class="tablinks" onclick="Sp_TabNav(event, 'ServicreScedule')" id="ServicreSceduleTabBtn"> Service Schedule </button>
        <button class="tablinks" onclick="Sp_TabNav(event, 'ServiceHistory')" id="ServiceHistoryTabBtn">Service History</button>
        <button class="tablinks" onclick="Sp_TabNav(event, 'MyRating')" id="MyRatingTabBtn">My Rating</button>
        <button class="tablinks " onclick="Sp_TabNav(event, 'BlockCustomer')" id="BlockCustomerTabBtn">Block Customer</button>
        <button class="tablinks " onclick="Sp_TabNav(event, 'Invoice')" id="InvoiceTabBtn">Invoice</button>
        <button class="tablinks " onclick="Sp_TabNav(event, 'Notification')" id="SettingsTabBtn">Notification</button>


    </div>

    <!-- table -->
    @await Html.PartialAsync("UpcomingService")
    @await Html.PartialAsync("ServiceHistory")
    @await Html.PartialAsync("ServiceRequest")
    @await Html.PartialAsync("Rating")
    @await Html.PartialAsync("BlockCustomer")
    @await Html.PartialAsync("Setting1")
    @await Html.PartialAsync("ServiceScedule")




</section>










<!-- script -->



<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

