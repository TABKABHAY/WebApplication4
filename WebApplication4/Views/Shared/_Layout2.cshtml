@{
}
<html>
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <!--Custome Css-->


    <link rel="stylesheet" href="/css/customerDashboard.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">



    <link rel="stylesheet" href="/css/navbar.css">


    <!--JS-->
    <script src="/js/faq.js"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="~/css/hederfooter.css">
    <link rel="image" href="~/image">
    <link rel="script" href="~/js/">
</head>
<body>
    <!-- !PAGE CONTENT header -->
    <header class="hdr1">

        <div class="head">
            <div class="imhlogo"><a asp-controller="Home" asp-action="Index"> <img src='~/image/logo-large.png' alt="logo" class="logo"> </a></div>

            @if (ViewBag.Name == null)
            {
                <div class="leftblock">
                    <a class="login" asp-controller="Home" asp-action="BookingService">bookcleaner</a>
                    <a class="pargaraph" asp-controller="Home" asp-action="Price">prices</a>
                    <a class="pargaraph">ourguarantee</a>
                    <a class="pargaraph">blog</a>
                    <a class="pargaraph" asp-controller="Home" asp-action="Contact">Contact</a>
                    <a class="login" onclick="document.getElementById('id01').style.display='block'">Logout</a>
                    <a class="login" asp-controller="Home" asp-action="Service_Provider_Become_a_Pro">Become Helper</a>
                </div>

            }

            <div class="hembg">
                <button onclick="w3_open()">☰</button>
            </div>
        </div>
        <!-- !PAGE CONTENT header -->
        <!-- Sidebar -->
        <div class="nevdata" onclick="document.getElementById('nevdata').style.display='none'">
            <div class="w3-sidebar w3-bar-block w3-border-right" id="mySidebar" style="display:none;">
                <button onclick="w3_close()" class="w3-bar-item w3-large">Close  × </button>
                <a class="w3-bar-item w3-button" asp-controller="Home" asp-action="BookingService">bookcleaner</a>
                <a class="w3-bar-item w3-button" asp-controller="Home" asp-action="Price">prices</a>
                <a class="w3-bar-item w3-button">ourguarantee</a>
                <a class="w3-bar-item w3-button">blog</a>
                <a class="w3-bar-item w3-button" asp-controller="Home" asp-action="Contact">Contact</a>


                <a class="w3-bar-item w3-button" onclick="document.getElementById('id01').style.display='block'">Login</a>
                <a class="w3-bar-item w3-button" asp-controller="Home" asp-action="Service_Provider_Become_a_Pro">becomehelper</a>
            </div>
        </div>
        <!-- Page Content -->
    </header>

    <div>
        @RenderBody()
    </div>

    <!---!PAGE CONTENTFOOTER!-->
    <footer class="helpfooter">
        <div class="Rectangle-12">
            <div class="ftrnav">
                <a asp-controller="Home" asp-action="Index"><img class="smalllogo" src="~/image/logo-small.png"></a>
                <div class="homeabout">
                    <a asp-controller="Home" asp-action="Index"> HOME</a>
                    <a asp-controller="Home" asp-action="About">About</a>
                    <a href="" class="">TESTIMONIALS</a>
                    <a asp-controller="Home" asp-action="faq">FAQ</a>
                    <a href="" class="">INSURANCE POLICY</a>
                    <a href="" class="">IMPRESSUM</a>
                    <a href="" class="">INSURANCE</a>
                </div>
                <div class="social">
                    <img class="insta" src="~/image/ic-instagram.png" alt="">
                    <img class="facebook" src="~/image/ic-facebook.png" alt="">
                </div>
            </div>
        </div>
        <div class="privicy">
            <p class="privicypolicy">
                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Ut feugiat nunc libero, ac malesuada ligula
                aliquam ac.<a href="">
                    Privacy Policy
                </a>
            </p>
            <button class="ok">OK!</button>
        </div>
    </footer>
    <script src="~/js/home.js"></script>
</body>

</html>